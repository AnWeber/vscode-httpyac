{
  "title": "HTTP Client Environment JSON schema",
  "$schema": "https://json-schema.org/draft-04/schema#",
  "type": "object",
  "patternProperties": {
    ".+": {
      "type": "object",
      "patternProperties": {
        ".+": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "string"
            }
          ],
          "description": "User defined variable"
        }
      },
      "description": "Environment"
    }
  }
}